   *            A value from 0 to yylength()-1.
   *
   * @return the character at position pos
   */
  public final char yycharat(int pos) {
    return zzBuffer[zzStartRead+pos];
  }


  /**
   * Returns the length of the matched text region.
   */
  public final int yylength() {
    return zzMarkedPos-zzStartRead;
  }


  /**
   * Reports an error that occured while scanning.
   *
   * In a wellformed scanner (no or only correct usage of 
   * yypushback(int) and a match-all fallback rule) this method 
   * will only be called with things that "Can't Possibly Happen".
   * If this method is called, something is seriously wrong
   * (e.g. a JFlex bug producing a faulty scanner etc.).
   *
   * Usual syntax/scanner level error handling should be done
   * in error fallback rules.
   *
   * @param   errorCode  the code of the errormessage to display
   */
  private void zzScanError(int errorCode) {
    String message;
    try {
      message = ZZ_ERROR_MSG[errorCode];
    }
    catch (ArrayIndexOutOfBoundsException e) {
      message = ZZ_ERROR_MSG[ZZ_UNKNOWN_ERROR];
    }

    throw new Error(message);
  } 


  /**
   * Pushes the specified amount of characters back into the input stream.
   *
   * They will be read again by then next call of the scanning method
   *
   * @param number  the number of characters to be read again.
   *                This number must not be greater than yylength()!
   */
  public void yypushback(int number)  {
    if ( number > yylength() )
      zzScanError(ZZ_PUSHBACK_2BIG);

    zzMarkedPos -= number;
  }


  /**
   * Contains user EOF-code, which will be executed exactly once,
   * when the end of file is reached
   */
  private void zzDoEOF() throws java.io.IOException {
    if (!zzEOFDone) {
      zzEOFDone = true;
      yyclose();
    }
  }


  /**
   * Resumes scanning until the next regular expression is matched,
   * the end of input is encountered or an I/O-Error occurs.
   *
   * @return      the next token
   * @exception   java.io.IOException  if any I/O-Error occurs
   */
  public java_cup.runtime.Symbol next_token() throws java.io.IOException {
    int zzInput;
    int zzAction;

    // cached fields:
    int zzCurrentPosL;
    int zzMarkedPosL;
    int zzEndReadL = zzEndRead;
    char [] zzBufferL = zzBuffer;
    char [] zzCMapL = ZZ_CMAP;

    int [] zzTransL = ZZ_TRANS;
    int [] zzRowMapL = ZZ_ROWMAP;
    int [] zzAttrL = ZZ_ATTRIBUTE;

    while (true) {
      zzMarkedPosL = zzMarkedPos;

      zzAction = -1;

      zzCurrentPosL = zzCurrentPos = zzStartRead = zzMarkedPosL;
  
      zzState = ZZ_LEXSTATE[zzLexicalState];

      // set up zzAction for empty match case:
      int zzAttributes = zzAttrL[zzState];
      if ( (zzAttributes & 1) == 1 ) {
        zzAction = zzState;
      }


      zzForAction: {
        while (true) {
    
          if (zzCurrentPosL < zzEndReadL) {
            zzInput = Character.codePointAt(zzBufferL, zzCurrentPosL, zzEndReadL);
            zzCurrentPosL += Character.charCount(zzInput);
          }
          else if (zzAtEOF) {
            zzInput = YYEOF;
            break zzForAction;
          }
          else {
            // store back cached positions
            zzCurrentPos  = zzCurrentPosL;
            zzMarkedPos   = zzMarkedPosL;
            boolean eof = zzRefill();
            // get translated positions and possibly new buffer
            zzCurrentPosL  = zzCurrentPos;
            zzMarkedPosL   = zzMarkedPos;
            zzBufferL      = zzBuffer;
            zzEndReadL     = zzEndRead;
            if (eof) {
              zzInput = YYEOF;
              break zzForAction;
            }
            else {
              zzInput = Character.codePointAt(zzBufferL, zzCurrentPosL, zzEndReadL);
              zzCurrentPosL += Character.charCount(zzInput);
            }
          }
          int zzNext = zzTransL[ zzRowMapL[zzState] + zzCMapL[zzInput] ];
          if (zzNext == -1) break zzForAction;
          zzState = zzNext;

          zzAttributes = zzAttrL[zzState];
          if ( (zzAttributes & 1) == 1 ) {
            zzAction = zzState;
            zzMarkedPosL = zzCurrentPosL;
            if ( (zzAttributes & 8) == 8 ) break zzForAction;
          }

        }
      }

      // store back cached position
      zzMarkedPos = zzMarkedPosL;

      if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
        zzAtEOF = true;
            zzDoEOF();
          { return new java_cup.runtime.Symbol(sym.EOF); }
      }
      else {
        switch (zzAction < 0 ? zzAction : ZZ_ACTION[zzAction]) {
          case 1: 
            { throw new Error("ilegal char: "+yytext());
            } 
            // fall through
          case 45: break;
          case 2: 
            { return new Symbol(sym.IDENT, yytext());
            } 
            // fall through
          case 46: break;
          case 3: 
            { return new Symbol(sym.AP);
            } 
            // fall through
          case 47: break;
          case 4: 
            { return new Symbol(sym.CP);
            } 
            // fall through
          case 48: break;
          case 5: 
            { return new Symbol(sym.AC);
            } 
            // fall through
          case 49: break;
          case 6: 
            { return new Symbol(sym.CC);
            } 
            // fall through
          case 50: break;
          case 7: 
            { return new Symbol(sym.ALL);
            } 
            // fall through
          case 51: break;
          case 8: 
            { return new Symbol(sym.CLL);
            } 
            // fall through
          case 52: break;
          case 9: 
            { return new Symbol(sym.MAS);
            } 
            // fall through
          case 53: break;
          case 10: 
            { return new Symbol(sym.MENOS);
            } 
            // fall through
          case 54: break;
          case 11: 
            { return new Symbol(sym.POR);
            } 
            // fall through
          case 55: break;
          case 12: 
            { return new Symbol(sym.DIV);
            } 
            // fall through
          case 56: break;
          case 13: 
            { return new Symbol(sym.ASIG);
            } 
            // fall through
          case 57: break;
          case 14: 
            { return new Symbol(sym.PYC);
            } 
            // fall through
          case 58: break;
          case 15: 
            { return new Symbol(sym.COMA);
            } 
            // fall through
          case 59: break;
          case 16: 
            { return new Symbol(sym.NEG);
            } 
            // fall through
          case 60: break;
          case 17: 
            { return new Symbol(sym.MENOR);
            } 
            // fall through
          case 61: break;
          case 18: 
            { return new Symbol(sym.MAYOR);
            } 
            // fall through
          case 62: break;
          case 19: 
            { return new Symbol(sym.PUNTO);
            } 
            // fall through
          case 63: break;
          case 20: 
            { return new Symbol(sym.ENTERO, yytext());
            } 
            // fall through
          case 64: break;
          case 21: 
            { 
            } 
            // fall through
          case 65: break;
          case 22: 
            { return new Symbol(sym.IF);
            } 
            // fall through
          case 66: break;
          case 23: 
            { return new Symbol(sym.DO);
            } 
            // fall through
          case 67: break;
          case 24: 
            { return new Symbol(sym.IGUAL);
            } 
            // fall through
          case 68: break;
          case 25: 
            { return new Symbol(sym.DISTINTO);
            } 
            // fall through
          case 69: break;
          case 26: 
            { return new Symbol(sym.MENOROIGUAL);
            } 
            // fall through
          case 70: break;
          case 27: 
            { return new Symbol(sym.MAYOROIGUAL);
            } 
            // fall through
          case 71: break;
          case 28: 
            { return new Symbol(sym.AND);
            } 
            // fall through
          case 72: break;
          case 29: 
            { return new Symbol(sym.OR);
            } 
            // fall through
          case 73: break;
          case 30: 
            { return new Symbol(sym.STRINGTEXT, yytext());
            } 
            // fall through
          case 74: break;
          case 31: 
            { return new Symbol(sym.INT);
            } 
            // fall through
          case 75: break;
          case 32: 
            { return new Symbol(sym.FOR);
            } 
            // fall through
          case 76: break;
          case 33: 
            { return new Symbol(sym.REAL, yytext());
            } 
            // fall through
          case 77: break;
          case 34: 
            { return new Symbol(sym.CHARTEXT, yytext());
            } 
            // fall through
          case 78: break;
          case 35: 
            { return new Symbol(sym.ELSE);
            } 
            // fall through
          case 79: break;
          case 36: 
            { return new Symbol(sym.CHAR);
            } 
            // fall through
          case 80: break;
          case 37: 
            { return new Symbol(sym.FLOAT);
            } 
            // fall through
          case 81: break;
          case 38: 
            { return new Symbol(sym.WHILE);
            } 
            // fall through
          case 82: break;
          case 39: 
            { return new Symbol(sym.PRINT);
            } 
            // fall through
          case 83: break;
          case 40: 
            { return new Symbol(sym.CINT);
            } 
            // fall through
          case 84: break;
          case 41: 
            { return new Symbol(sym.LENGTH);
            } 
            // fall through
          case 85: break;
          case 42: 
            { return new Symbol(sym.STRING);
            } 
            // fall through
          case 86: break;
          case 43: 
            { return new Symbol(sym.CCHAR);
            } 
            // fall through
          case 87: break;
          case 44: 
            { return new Symbol(sym.CFLOAT);
            } 
            // fall through
          case 88: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }


}
